{
  "extends": "airbnb",
  "rules": {
    "react/prop-types": 0,
    "linebreak-style": "off",
    "arrow-parens": "off",
    "react/no-multi-comp": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "react/prefer-stateless-function": "off",
    "wrap-iife": "off"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "google": true
  }
}
